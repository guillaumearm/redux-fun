{
  "name": "redux-fun",
  "version": "1.1.6",
  "description": "Redux functional programming utilities",
  "main": "./src/index.js",
  "scripts": {
    "clean": "rimraf ./coverage",
    "bump": "conventional-bump -p angular",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "commit": "git-cz",
    "test": "npm run lint",
    "test:all": "npm run test",
    "lint": "eslint .",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "preversion": "npm run test:all",
    "prepublishOnly": "npm run test:all",
    "precommit": "lint-staged",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guillaumearm/redux-fun.git"
  },
  "bugs": {
    "url": "https://github.com/guillaumearm/redux-fun/issues"
  },
  "keywords": [
    "redux",
    "functional-programming",
    "fp",
    "middleware",
    "selector",
    "reducer",
    "updater"
  ],
  "author": "Guillaume ARM <trapcodien@hotmail.fr> (https://github.com/guillaumearm/)",
  "license": "MIT",
  "homepage": "https://github.com/guillaumearm/redux-fun",
  "devDependencies": {
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2",
    "commitizen": "^2.9.6",
    "conventional-bump": "^1.0.2",
    "conventional-changelog-cli": "^1.3.5",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.12.1",
    "eslint-plugin-node": "^5.2.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "lodash.curry": "^4.1.1",
    "lodash.mapvalues": "^4.6.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
