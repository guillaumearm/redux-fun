{
  "name": "redux-fun",
  "version": "1.1.2",
  "description": "Redux functional programming utilities",
  "main": "./src/index.js",
  "scripts": {
    "clean": "rimraf ./coverage",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "commit": "git-cz",
    "test": "npm run lint",
    "test:all": "npm run test",
    "lint": "eslint .",
    "preversion": "npm run test:all",
    "prepublish": "npm run test:all",
    "precommit": "lint-staged",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guillaumearm/redux-fun.git"
  },
  "keywords": [
    "redux",
    "fp",
    "functional",
    "programming"
  ],
  "author": "Guillaume ARM",
  "license": "MIT",
  "homepage": "https://github.com/guillaumearm/redux-fun",
  "devDependencies": {
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.12.1",
    "eslint-plugin-node": "^5.2.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "lodash.curry": "^4.1.1",
    "lodash.mapvalues": "^4.6.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
